# CompilerName=clang
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/maci64
matlabdir = /Applications/MATLAB_R2021a.app
startdir = /Users/kimberlychan/Library/CloudStorage/GoogleDrive-kchan45@berkeley.edu/My Drive/Research/dnn_generality/DNN_Generality/matlab

# Toolchain information
cc = /usr/bin/xcrun -sdk macosx12.3 clang
cxx = /usr/bin/xcrun -sdk macosx12.3 clang++
linker = /usr/bin/xcrun -sdk macosx12.3 clang
linkerxx = /usr/bin/xcrun -sdk macosx12.3 clang++

cflags =  -fno-common -arch x86_64 -mmacosx-version-min=10.14 -fexceptions -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX12.3.sdk -DMATLAB_MEX_FILE -O2 -fwrapv -DNDEBUG -std=c99 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=plasma_nn_wrapper_fixpt_mex -c 

cxxflags =  -fno-common -arch x86_64 -mmacosx-version-min=10.14 -fexceptions -isysroot /Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX12.3.sdk -fobjc-arc -std=c++11 -stdlib=libc++ -DMATLAB_MEX_FILE -O2 -fwrapv -DNDEBUG -std=c++11 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=plasma_nn_wrapper_fixpt_mex -c 

ldflags =  -Wl,-twolevel_namespace -undefined error -arch x86_64 -mmacosx-version-min=10.14 -Wl,-syslibroot,/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX12.3.sdk -bundle   -L"$matlabdir/bin/maci64" -lmx -lmex -lmat -lc++  -Wl,-rpath,@loader_path -o plasma_nn_wrapper_fixpt_mex.mexmaci64 -lemlrt -lcovrt -lut -lmwmathutil  -Wl,-exported_symbols_list,plasma_nn_wrapper_fixpt_mex.map

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker $in $ldflags

rule linkxx
    command = $linkerxx $in $ldflags

# Build
build $builddir/plasma_nn_wrapper_fixpt_data.o : cc $root/plasma_nn_wrapper_fixpt_data.c
build $builddir/rt_nonfinite.o : cc $root/rt_nonfinite.c
build $builddir/plasma_nn_wrapper_fixpt_initialize.o : cc $root/plasma_nn_wrapper_fixpt_initialize.c
build $builddir/plasma_nn_wrapper_fixpt_terminate.o : cc $root/plasma_nn_wrapper_fixpt_terminate.c
build $builddir/plasma_nn_wrapper_fixpt.o : cc $root/plasma_nn_wrapper_fixpt.c
build $builddir/_coder_plasma_nn_wrapper_fixpt_api.o : cc $root/interface/_coder_plasma_nn_wrapper_fixpt_api.c
build $builddir/_coder_plasma_nn_wrapper_fixpt_mex.o : cc $root/interface/_coder_plasma_nn_wrapper_fixpt_mex.c
build $builddir/_coder_plasma_nn_wrapper_fixpt_info.o : cc $root/interface/_coder_plasma_nn_wrapper_fixpt_info.c
build $builddir/c_mexapi_version.o : cc $matlabdir/extern/version/c_mexapi_version.c

# Link
build $root/plasma_nn_wrapper_fixpt_mex.mexmaci64 : link $builddir/plasma_nn_wrapper_fixpt_data.o $builddir/rt_nonfinite.o $builddir/plasma_nn_wrapper_fixpt_initialize.o $builddir/plasma_nn_wrapper_fixpt_terminate.o $builddir/plasma_nn_wrapper_fixpt.o $builddir/_coder_plasma_nn_wrapper_fixpt_api.o $builddir/_coder_plasma_nn_wrapper_fixpt_mex.o $builddir/_coder_plasma_nn_wrapper_fixpt_info.o $builddir/c_mexapi_version.o 
